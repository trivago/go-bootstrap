{
  "include-component-in-tag": false,
  "include-v-in-tag": true,
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features",
      "hidden": false
    },
    {
      "type": "fix",
      "section": "Bug Fixes",
      "hidden": false
    },
    {
      "type": "chore",
      "section": "Miscellaneous",
      "hidden": false
    }
  ],
  "exclude-paths": [
    "hack",
    ".github"
  ],
  "packages": {
    ".": {
      "package-name": "go-boostrap",
      "changelog-path": "CHANGELOG.md",
      "release-type": "go"
    }
  }
}
